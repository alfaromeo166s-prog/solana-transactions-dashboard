{
  "mint": "99zqUzjGohamrYygoyXTEb191iom3LCimxCA75FX5Tg",
  "decimals": 9,
  "signature": "44fwkT1EzBlwudzrCy3fvCrbLwMbrVbtsd760ZyE6cKaegNupBpq"
}use std::fs;
use serde::Deserialize;

#[derive(Debug, Deserialize, Serialize)]
struct TokenInfo {
    mint: String,
    decimals: u8,
    signature: String,
}

async fn get_token_info() -> Json<TokenInfo> {
    let content = fs::read_to_string("backend/tokens.json").unwrap();
    let token: TokenInfo = serde_json::from_str(&content).unwrap();
    Json(token)
}
